export function parseAddress(address: string): ParsedAddress | null {
  if (!address) return null;
  
  const addressLower = address.toLowerCase();
  const parsed: ParsedAddress = {};

  // Step 1: 약칭 정규화 시도 (우선순위 높음)
  const normalized = normalizeLocationAlias(address);
  if (normalized.city) {
    parsed.city = normalized.city;
    parsed.district = normalized.district;
    parsed.neighborhood = normalized.microPoi;
    return parsed;
  }

  // Step 2: 정규식 기반 파싱 (기존 로직)
  for (const city of Object.keys(locationDatabase)) {
    if (addressLower.includes(city.toLowerCase())) {
      parsed.city = city;
      break;
    }
  }

  if (parsed.city) {
    for (const district of Object.keys(locationDatabase[parsed.city])) {
      if (addressLower.includes(district.toLowerCase())) {
        parsed.district = district;
        break;
      }
    }
  }

  const neighborhoods = ['동', '읍', '면', '리'];
  const words = address.split(/[\s,\-]/);
  for (const word of words) {
    for (const suffix of neighborhoods) {
      if (word.endsWith(suffix) && word.length > 1) {
        parsed.neighborhood = word;
        break;
      }
    }
  }

  // 도로명 파싱 (로, 길, 로길, 로타길 등)
  const streetPattern = /([가-힣]+(?:로|길|로길|타길|타로))\s*(\d+)?(?:길)?\s*(\d+)?/;
  const streetMatch = address.match(streetPattern);
  if (streetMatch) {
    parsed.street = streetMatch[0];
  }

  return parsed;
}

export function extractLocationWithPriority(
  placeInfo: string,
  description: string
): LocationExtractionResult {
  // Step 1: 약칭 정규화 시도 (우선순위 가장 높음)
  const placeAlias = normalizeLocationAlias(placeInfo);
  const descAlias = normalizeLocationAlias(description);

  let city = '';
  let district = '';
  let neighborhoods: string[] = [];
  let source = 'default';

  // 약칭 매칭 우선 처리
  if (placeAlias.city) {
    city = placeAlias.city;
    district = placeAlias.district || '';
    if (placeAlias.microPoi) neighborhoods.push(placeAlias.microPoi);
    source = 'alias_placeInfo';
  } else if (descAlias.city) {
    city = descAlias.city;
    district = descAlias.district || '';
    if (descAlias.microPoi) neighborhoods.push(descAlias.microPoi);
    source = 'alias_description';
  } else {
    // Step 2: 정규식 기반 파싱 (약칭 매칭 실패 시)
    const descParsed = parseAddress(description);
    const placeParsed = parseAddress(placeInfo);

    city = descParsed?.city || placeParsed?.city || '전국';
    district = descParsed?.district || placeParsed?.district || '';
    source = descParsed?.city ? 'description' : placeParsed?.city ? 'placeInfo' : 'default';

    if (descParsed?.neighborhood) neighborhoods.push(descParsed.neighborhood);
    if (placeParsed?.neighborhood && !neighborhoods.includes(placeParsed.neighborhood)) {
      neighborhoods.push(placeParsed.neighborhood);
    }
  }

  // 신뢰도 결정
  const confidence =
    city && city !== '전국' && district
      ? 'high'
      : city && city !== '전국'
      ? 'medium'
      : 'low';

  return {
    city,
    district,
    neighborhoods,
    confidence,
    source,
    signals: {
      cityFound: !!city && city !== '전국',
      districtFound: !!district,
      neighborhoodsCount: neighborhoods.length
    }
  };
}

export function validateLocationConsistency(
  placeInfo: string,
  description: string
): { isConsistent: boolean; warnings: string[]; recommendation: string } {
  const warnings: string[] = [];
  return {
    isConsistent: warnings.length === 0,
    warnings,
    recommendation: 'OK'
  };
}

export function getNearbyLocations(city: string, district: string): NearbyLocations {
  const defaultResult: NearbyLocations = {
    commercialAreas: [],
    metro: [],
    landmarks: [],
    attractions: []
  };

  if (!city || !locationDatabase[city]) {
    return defaultResult;
  }

  const cityData = locationDatabase[city];
  if (!district || !cityData[district]) {
    return defaultResult;
  }

  const districtData = cityData[district];
  return {
    commercialAreas: districtData.commercialAreas || [],
    metro: districtData.metro || [],
    landmarks: districtData.landmarks || [],
    attractions: districtData.attractions || []
  };
}

